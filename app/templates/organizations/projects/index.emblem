#projects.container
  button.new-project-button click="openNewProjectModal" id="newProject" New Project

  h1 Projects
  section.projects
    = each project in content
      div.project-listing
        project.name
        project.client
        project.description
        = time-format project.start_date 'l'
        project.duration_weeks
        div.project-buttons
          button{ action "openEditProjectModal" project.id on="click" } Edit
          button{ action "openDestroyProjectModal" project.id on="click" } Delete
